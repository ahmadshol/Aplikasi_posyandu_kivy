#:kivy 2.3.0

<ClickableImage>:
    allow_stretch: True
    size_hint_x: 0.1
    size: '30dp', '30dp'

<MyScreenManager>:
    LoginScreen:
    LoginSecondScreen: 
    HomeApp:
    RegistrationScreen: 
    AccountScreen:


<HomeApp>:
    name: 'home'

    BoxLayout:  # Ganti HealthApp menjadi BoxLayout
        orientation: 'vertical'
        padding: '10dp'
        spacing: '10dp'
        canvas.before:
            Color:
                rgba: 1, 1, 1, 1  # Set background color to white
            Rectangle:
                pos: self.pos
                size: self.size

        # Top bar with time, app name, notification, and profile icons
        BoxLayout:
            size_hint_y: None
            height: '50dp'
            padding: '5dp'
            Label:
                text: '9:27'
                size_hint_x: 0.1
            Label:
                text: 'Save Health'
                font_size: '24dp'
                bold: True
            ClickableImage:
                source: 'img/location (1) 1.png' 
                on_press:
                    app.root.current = 'page_two' 

            ClickableImage:
                source: 'img/avatar.png' 
                on_press:
                    app.root.current = 'page_one'

                    
        # Search bar
        BoxLayout:
            size_hint_y: None
            height: '40dp'
            TextInput:
                hint_text: 'Search'
                multiline: False

        # Column for icons and "Konsultasi dengan spesialis" inside one box
        BoxLayout:
            orientation: 'vertical'
            size_hint_y: None
            height: '250dp'
            padding: '5dp'
            canvas.before:
                Color:
                    rgba: 0.9, 0.9, 0.9, 1
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [10,]
            GridLayout:
                cols: 5
                padding: '10dp'
                spacing: '5dp'
                # Icons and text below them
                BoxLayout:
                    orientation: 'vertical'
                    Image:
                        source: 'img/konsultasi.png'
                        size_hint: None, None
                        size: '50dp', '50dp'
                        allow_stretch: True  # Ensure the image stretches
                    Label:
                        text: 'Konsultasi'
                        font_size: '12dp'
                        halign: 'center'
                        color: 0, 0, 0, 1
                BoxLayout:
                    orientation: 'vertical'
                    Image:
                        source: 'img/location (1) 1.png'
                        size_hint: None, None
                        size: '50dp', '50dp'
                        allow_stretch: True  # Ensure the image stretches
                    Label:
                        text: 'Lokasi'
                        font_size: '12dp'
                        halign: 'center'
                        color: 0, 0, 0, 1
                BoxLayout:
                    orientation: 'vertical'
                    Image:
                        source: 'img/imun 1.png'
                        size_hint: None, None
                        size: '50dp', '50dp'
                        allow_stretch: True  # Ensure the image stretches
                    Label:
                        text: 'Imunisasi'
                        font_size: '12dp'
                        halign: 'center'
                        color: 0, 0, 0, 1
                BoxLayout:
                    orientation: 'vertical'
                    Image:
                        source: 'img/pendafftaran.png'
                        size_hint: None, None
                        size: '50dp', '50dp'
                        allow_stretch: True  # Ensure the image stretches
                    Label:
                        text: 'Pendaftaran'
                        font_size: '12dp'
                        halign: 'center'
                        color: 0, 0, 0, 1
                BoxLayout:
                    orientation: 'vertical'
                    Image:
                        source: 'img/antrian 1.png'
                        size_hint: None, None
                        size: '50dp', '50dp'
                        allow_stretch: True  # Ensure the image stretches
                    Label:
                        text: 'Antrian'
                        font_size: '12dp'
                        halign: 'center'
                        color: 0, 0, 0, 1

            # Section for "Konsultasi dengan spesialis"
            BoxLayout:
                orientation: 'horizontal'
                padding: '10dp'
                spacing: '10dp'
                size_hint_y: None
                height: '50dp'
                canvas.before:
                    Color:
                        rgba: 0.8, 0.8, 0.8, 1
                    RoundedRectangle:
                        size: self.size
                        pos: self.pos
                        radius: [10,]
                Image:
                    source: 'img/konsultasi.png'
                    size_hint_x: None
                    width: '40dp'
                    allow_stretch: True
                Label:
                    text: 'Konsultasi dengan spesialis'
                    font_size: '16dp'
                    bold: True
                    color: 0, 0, 0, 1
                Button:
                    text: '>'
                    size_hint_x: None
                    width: '30dp'
                    background_color: (0, 0.5, 1, 1)
                    color: 1, 1, 1, 1

        # User profiles list aligned with avatars and text
        GridLayout:
            cols: 2
            size_hint_y: None
            height: '180dp'
            padding: '10dp'
            spacing: '10dp'
            
            # First user (Rohmad Rafi N)
            Image:
                source: 'img/avatar.png'
                size_hint: None, None
                size: '50dp', '50dp'
                allow_stretch: True  # Ensure the image stretches
            BoxLayout:
                orientation: 'vertical'
                Label:
                    text: 'Rohmad Rafi N'
                    font_size: '16dp'
                    halign: 'left'
                    valign: 'middle'
                    text_size: self.size
                    color: 0, 0, 0, 1
                Label:
                    text: 'Lansia'
                    font_size: '12dp'
                    halign: 'left'
                    valign: 'middle'
                    text_size: self.size
                    color: 0, 0, 0, 1
            
            # Second user (Musarof Morisrona)
            Image:
                source: 'img/avatar.png'
                size_hint: None, None
                size: '50dp', '50dp'
                allow_stretch: True  # Ensure the image stretches
            BoxLayout:
                orientation: 'vertical'
                Label:
                    text: 'Musarof Morisrona'
                    font_size: '16dp'
                    halign: 'left'
                    valign: 'middle'
                    text_size: self.size
                    color: 0, 0, 0, 1
                Label:
                    text: 'Balita'
                    font_size: '12dp'
                    halign: 'left'
                    valign: 'middle'
                    text_size: self.size
                    color: 0, 0, 0, 1

        # Add button
        AnchorLayout:
            anchor_x: 'center'
            anchor_y: 'bottom'
            Button:
                text: '+'
                size_hint: None, None
                size: '50dp', '50dp'
                background_normal: ''
                background_color: 0.2, 0.6, 0.4, 1
                color: 1, 1, 1, 1
                font_size: '40dp'



<LoginScreen>:
    name: 'page_one'

    BoxLayout:
        orientation: 'vertical'
        padding: [20, 20, 20, 20]
        spacing: 10

        canvas.before:
            Color:
                rgba: 1, 1, 1, 1
            Rectangle:
                pos: self.pos
                size: self.size

        Widget:
            size_hint_x: 0.08

        Label:
            text: "Aplikasi\nKesehatan\nMasyarakat"
            font_size: 28
            size_hint_y: None
            height: 135
            halign: 'center'
            valign: 'middle'
            text_size: self.size
            color: 0, 0, 0, 1

        # BoxLayout for buttons to keep them in a column
        BoxLayout:
            orientation: 'vertical'
            spacing: 10
            BoxLayout:
                orientation: 'horizontal'
                size_hint: None, None
                size: 300, 40
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                spacing: -120


                # Menggambar background rounded kuning
                canvas.before:
                    Color:
                        rgba: 1, 0.8, 0, 1  # Warna kuning
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
                        radius: [20]  # Membuat sudut membulat

                Image:
                    source: 'img/facebook.jpg'
                    size_hint: 0.5 , 0.5
                    size: 30, 30
                    pos_hint: {'center_y': 0.5 ,'center_x': 0.5}   # Memastikan ikon di tengah vertikal

                Button:
                    size_hint_y: None
                    pos_hint: {'center_y': 0.5 ,'center_x': 0.5}
                    height: 50
                    text: "Continue with Facebook"
                    background_normal: ''
                    background_color: 1, 1, 1, 0  # Transparan agar warna background dari canvas terlihat
                    color: 0, 0, 0, 1  # Warna teks hitam
                    on_release: app.root.current = 'login'

            BoxLayout:
                orientation: 'horizontal'
                size_hint: None, None
                size: 300, 40
                pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                spacing: -120


                # Menggambar background rounded kuning
                canvas.before:
                    Color:
                        rgba: 1, 0.8, 0, 1  # Warna kuning
                    RoundedRectangle:
                        pos: self.pos
                        size: self.size
                        radius: [20]  # Membuat sudut membulat

                Image:
                    source: 'img/logogoogle.jpg'
                    size_hint: 0.5 , 0.5
                    size: 30, 30
                    pos_hint: {'center_y': 0.5 ,'center_x': 0.5}   # Memastikan ikon di tengah vertikal

                Button:
                    size_hint_y: None
                    pos_hint: {'center_y': 0.5 ,'center_x': 0.5}
                    height: 50
                    text: "Continue with Google"
                    background_normal: ''
                    background_color: 1, 1, 1, 0  # Transparan agar warna background dari canvas terlihat
                    color: 0, 0, 0, 1  # Warna teks hitam
                    on_release: app.root.current = 'page_two'

        Button:
            text: "Registrasi / Membuat Akun"
            size_hint_y: None
            height: 30
            background_normal: ''
            background_color: (0, 0, 0, 0)
            color: (0, 0, 1, 1)
            italic: True
            on_press: app.root.current = 'registrasi'

        Button:
            text: "Masuk sebagai tamu"
            size_hint_y: None
            height: 50
            background_normal: ''
            background_color: (0.5, 0.5, 0.5, 1)
            color: (0, 0, 0, 1)
            on_press: root.guest_login()

        Widget:
            size_hint_x: 0.08

        BoxLayout:
            orientation: 'horizontal'
            size_hint_y: None
            height: 30
            spacing: 10
            pos_hint: {'center_x': .5}

            Button:
                text: "Ketentuan Layanan"
                color:(0,0,1,1)
                background_normal:''
                background_color:(0,0,0,0)
                on_press : print("Ketentuan Layanan clicked")

            Button:
                text:"Kebijakan Privasi"
                color:(0 ,0 ,1 ,1)
                background_normal:''
                background_color:(0 ,0 ,0 ,0)
                on_press : print("Kebijakan Privasi clicked")


