            rgba: 1, 1, 1, 1  # Background color for the whole app
        Rectangle:
            pos: self.pos
            size: self.size

    # Title
    BoxLayout:
        size_hint_y: None
        height: 50
        padding: 5
        spacing: 10
        size_hint_y: 1
        canvas.before:
            Color:
                rgba: 0.2, 0.6, 0.86, 1  # Background color for title
            Rectangle:
                pos: self.pos
                size: self.size
        Label:
            text: 'Data Lansia'
            font_size: '24sp'
            bold: True
            color: 1, 1, 1, 1  # White text color

    # Profile section
    BoxLayout:
        orientation: 'horizontal'
        size_hint_y: None
        height: 100
